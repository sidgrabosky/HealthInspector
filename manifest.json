{
  "name": "Health Inspector",
  "description" : "Puts health score badges onto delivery service websites for San Francisco restaurants.",
  "version": "1.0",
  "browser_action": {
    "default_popup": "healthscore.html",
    "default_icon": "images/health-inspector-256x256.png"
  },
  "manifest_version": 2,
  "permissions": [
    "https://data.sfgov.org/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.trycaviar.com/*"],
      "css": ["contentScriptStyles.css", "modal.css"],
      "js": ["scripts/contentScript.js"]
    }
  ]
}
